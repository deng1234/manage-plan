<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
   <class name="VO.Plan" table="plan">
      <id name="planID" type="int" column="planID">
         <generator class="native"/>
      </id>
      <property name="score" column="score" type="float"/>
      <property name="summary" column="summary" type="string"/>
      <property name="planTime" column="planTime" type="date"/>
   </class>
</hibernate-mapping>